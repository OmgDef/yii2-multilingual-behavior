{
    "name": "omgdef/yii2-multilingual-behavior",
    "description": "Port of the yii-multilingual-behavior for yii",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", " yii2-multilingual-behavior", " behavior", " multilingual", " i18"],
    "license": "BSD-4-Clause",
    "authors": [
        {
            "name": "OmgDef",
            "email": "omgdef@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "omgdef\\multilingual\\": "src"
        }
    },
    "require": {
        "yiisoft/yii2": "^2.0.38"
    },
    "require-dev": {
      "phpunit/phpunit": "4.6.*",
      "phpunit/dbunit": ">=1.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        },
        "fxp-asset": {
            "enabled": false
        }
    }
}
